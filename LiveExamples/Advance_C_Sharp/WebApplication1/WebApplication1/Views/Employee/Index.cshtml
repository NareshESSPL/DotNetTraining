@{
}

@model IEnumerable<WebApplication1.DTO.Employees>

  <h2>Employee Data</h2>

<p>
    @Html.ActionLink("create new Employee", "Create", "Employee", null, new { @class = "btn btn-primary" })
<p />

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>EmployeesId</th>
            <th>FullName</th>
            <th>Age</th>
            <th>Gender</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var Employees in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m => Employees.EmployeeId)</td>
                <td>@Html.DisplayFor(m => Employees.EmployeeName)</td>
                <td>@Html.DisplayFor(m => Employees.EmployeeAge)</td>
                <td>@Html.DisplayFor(m => Employees.EmployeeGender)</td>
            </tr>
        }
    </tbody>
</table>
