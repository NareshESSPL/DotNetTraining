@{
}
<h2>User List</h2>
<table class="table table-striped"> 
    <thead>
        <tr> <th>User ID</th> 
            <th>User Name</th>
            <th>Password</th>
            <th>Is Active</th> 
        </tr>
        </thead> 
        <tbody> @foreach (var user in Model)
        {
            <tr> 
                <td>@user.UserId</td> 
                <td>@user.UserName</td>
                <td>@user.Password</td> 
                <td>@(user.IsActive ? "Yes" : "No")</td>
            </tr>
        }
        </tbody> </table>