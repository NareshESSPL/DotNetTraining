@model Student

<h1>@Model.FirstName Details</h1>

<table class="table table-striped table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Student ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date of Birth</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>Address</th>
            
        </tr>
    </thead>
    <tbody>
        
            <tr>
                <td>@Html.DisplayFor(m => Model.StudentID)</td>
                <td>@Html.DisplayFor(m => Model.FirstName)</td>
                <td>@Html.DisplayFor(m => Model.LastName)</td>
                <td>@Html.DisplayFor(m => Model.DateOfBirth)</td>
                <td>@Html.DisplayFor(m => Model.Email)</td>
                <td>@Html.DisplayFor(m => Model.PhoneNumber)</td>
                <td>@Html.DisplayFor(m => Model.Address)</td>

            </tr>
        }
    </tbody>
</table>
 