@model List<ImageUpload>

<h2>Uploaded Images</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Name</th>
            <th>Image</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>
                    <img src="data:image/jpeg;base64,@Convert.ToBase64String(item.ImageData)" width="100" />
                </td>
            </tr>
        }
    </tbody>
</table>