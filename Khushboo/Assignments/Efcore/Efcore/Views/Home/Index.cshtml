@model List<Entity.User>

<h2>User List</h2>

@if (Model != null && Model.Count > 0)
{
    <table class="table">
        <thead>
            <tr>
                <th>User ID</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No users found in the database.</p>
}
